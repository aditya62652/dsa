#include <iostream>
using namespace std;

int main() {
   int a=4;
   int b=6;
   cout<< "a&b="<<(a&b)<<endl;
   cout<< "a|b="<<(a|b)<<endl;
   cout<< "~a="<<(~a)<<endl;
   cout<<"~b="<<(~b)<<endl;
   cout<<(20<<2)<<endl;
   cout<< (5<<2)<<endl;
   
   
   return 0;                  
}                             
          
          a&b=4
a|b=6
~a=-5
~b=-7
80
20
