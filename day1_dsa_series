# include<iostream>
using namespace std;
int main(){
	int a,b;
	cout<<"enter the value of a and b"<<endl;
	cin>>a >>b;
	int sum=a+b;
	cout<<sum<<" ";
	return 0;
}

enter the value of a and b
10 20
30 


#include<iostream>
using namespace std;
int main(){
    int p,r,t;
    cout<<"enter the value of p,r,t"<<endl;
    cin>>p>>r>>t;
    int si=(p*r*10)/100;
    cout<<"simple intrest is:"<<si<<endl;
    return 0;
    
}

enter the value of p,r,t
10 20 30
simple intrest is:20

#include<iostream>
using namespace std;
int main(){
    int a,b;
    cin>>a>>b;
    if(a>b){
        cout<<"A is greater...."<<endl;
    }
    else{
        cout<<"B is greater......"<<endl;
    }
    return 0;
}

15 20
B is greater......

#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cout<<"enter the value of a,b,c"<<endl;
    cin>>a>>b>>c;
    int avg=(a+b+c)/3;
    cout<<"average" <<avg<<" "<<endl;
    return 0;
}

enter the value of a,b,c
2
2
2
average2 
